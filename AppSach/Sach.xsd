                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT MaSach,TenLoai,TenSach,Anh,SoLuong,TenNXB,
	
	FORMAT(
	DonGia,'#,###'+' VNĐ') AS DonGia,MoTa,TenTG,CONVERT(varchar(10),NgayNhap,103)AS NgayNhap
	FROM dbSach--,dbLoai,dbNXB
	inner join dbLoai On dbSach.MaLoai = dbLoai.MaLoai
	inner join dbNXB On dbSach.MaNXB = dbNXB.MaNXB
	WHERE dbSach.TrangThai=1 and(
	lower(TenSach)like'%'+LOWER(trim(@key))+'%' or
	lower(SoLuong)like'%'+LOWER(trim(@key))+'%' or
	lower(DonGia)like'%'+LOWER(trim(@key))+'%' or
	lower(MoTa)like'%'+LOWER(trim(@key))+'%' or
	lower(TenTG)like'%'+LOWER(trim(@key))+'%' or
	lower(TenLoai)like'%'+LOWER(trim(@key))+'%' or
	lower(TenNXB)like'%'+LOWER(trim(@key))+'%' or

	lower(NgayNhap)like'%'+LOWER(trim(@key))+'%')
	order by MaSach</CommandText>
                    <Parameters>
